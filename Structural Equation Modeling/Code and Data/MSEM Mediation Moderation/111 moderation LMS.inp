
DATA:	
    FILE IS Pregame PTSD 021615.dat;

   

VARIABLE: NAMES ARE
   IDNUM CAPS_STATUS_R CAPS_STATUS_combined 
   SEX AGE RACE EDLEVEL TLEQ_SV_Dich 
    LEC_SV_Dich CRITBMSx CRITCMSx CRITDMSx 
    TLEQ_CE TLEQ_CFH PHQ_TOT PHQ_Severity2groups 
    bfi_E bfi_A 
    bfi_C bfi_N bfi_O DERS_TOTAL dmqsoc dmqcop 
    dmqenh dmqcnf DMQ_Total KEXtr KEXsl KEXae 
    KEXpe YAQ_TOT 
    YAQ_soc YAQ_cont YAQ_selfp YAQ_selfc YAQ_risk 
    YAQ_acoc YAQ_dep YAQ_blk drnkdays30 Hvydays Bngdays 
    Prgmdays nopgdays TOTDRK30 TOTPGEDRK TOTDRKPGD 
    TOTDRKNOPG totcons30 Totpgcons Totnopgcons 
    Sexual_Consequence Pregamer Day P_TALUS TALUS ALC 
    DRINKDAY PGDAY pgdALC nopgALC CONSd ycond PGCONS 
    NOPGCONS DDD DPGD DNPGD CDD CPGD CNPGD
    PTSD1 PTSD2 PTSD3;

    !DUMMY CODE FOR CAPS_STATUS_combined
    !                       PTSD1   PTSD2 PTSD3
    !Criteria A Negative    1       0      0
    !PTSD Negative          0       1      0
    !PTSD Positive          0       0      1
    
    !can also use PTSD1 by itself to compare
    !those with trauma to those without trauma

    MISSING = ALL (-999);

  USEVARIABLES ARE
      pgday
      CONSd
      bfi_c
      cbfi_c
      cconsd
    ;


  CLUSTER IS IDNUM;
  within is pgday bfi_c consd;
  between is cbfi_c cconsd;
  
  define: 
  cbfi_c = cluster_mean(bfi_c);
  cconsd = cluster_mean(consd);

  
ANALYSIS:
   TYPE IS TWOLEVEL RANDOM;
   algorithm=integration;
   
MODEL:
   %WITHIN%
   PD by pgday@1; pd@.7; pgday@.1;
   BC by bfi_c@1; bc@.7; bfi_c@.1;
   ARC by consd@1; arc@.7; consd@.1;
  
 pdXbc | PD xwith BC ;
  ARC on PD BC pdXbc;
   
   
   %BETWEEN%
   cconsd cbfi_c;
  cconsd on cbfi_c;
  [cconsd cbfi_c];
  
  
  OUTPUT: TECH1 TECH3;
                            
   